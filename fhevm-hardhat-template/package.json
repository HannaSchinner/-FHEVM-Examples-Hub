{
  "name": "fhevm-hardhat-template",
  "version": "1.0.0",
  "description": "Production-ready Hardhat template for FHEVM smart contracts",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "coverage": "hardhat coverage",
    "node": "hardhat node",
    "deploy:local": "hardhat run scripts/deploy.ts --network localhost",
    "deploy:sepolia": "hardhat run scripts/deploy.ts --network sepolia",
    "deploy:zama": "hardhat run scripts/deploy.ts --network zamaTestnet",
    "clean": "hardhat clean",
    "verify": "hardhat verify",
    "typecheck": "tsc --noEmit",
    "lint": "solhint 'contracts/**/*.sol'",
    "format": "prettier --write 'contracts/**/*.sol' 'test/**/*.ts' 'scripts/**/*.ts'"
  },
  "keywords": [
    "fhevm",
    "fhe",
    "zama",
    "privacy",
    "confidential-computing",
    "blockchain",
    "ethereum",
    "hardhat",
    "solidity",
    "smart-contracts"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@fhevm/hardhat-plugin": "0.0.1-3",
    "@fhevm/mock-utils": "^0.0.1-3",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.1.0",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.5",
    "@zama-fhe/relayer-sdk": "^0.1.2",
    "chai": "^4.3.7",
    "dotenv": "^16.3.1",
    "hardhat": "^2.19.0",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-gas-reporter": "^1.0.9",
    "mocha": "^10.4.0",
    "prettier": "^3.0.0",
    "solhint": "^4.0.0",
    "ts-node": "^10.9.1",
    "typechain": "^8.3.2",
    "@typechain/ethers-v6": "^0.5.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@fhevm/solidity": "^0.7.0",
    "@zama-fhe/oracle-solidity": "^0.1.0",
    "ethers": "^6.14.0"
  }
}
