# Network Configuration
# Copy this file to .env and fill in your actual values
# Never commit .env to version control!

# Private key for deployment (without 0x prefix)
# IMPORTANT: Use a testnet-only wallet, never use mainnet private keys
PRIVATE_KEY=your_private_key_here

# RPC URLs for different networks
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY
ETHEREUM_RPC_URL=https://mainnet.infura.io/v3/YOUR_INFURA_KEY
ZAMA_TESTNET_RPC_URL=https://devnet.zama.ai

# Etherscan API key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# FHEVM Configuration
# Gateway URL for FHEVM operations
GATEWAY_URL=https://gateway.zama.ai

# Network Chain IDs
SEPOLIA_CHAIN_ID=11155111
ZAMA_TESTNET_CHAIN_ID=8009

# Gas Configuration (optional)
GAS_PRICE=20000000000
GAS_LIMIT=8000000

# Frontend Configuration (if using web interface)
REACT_APP_CONTRACT_ADDRESS=your_deployed_contract_address
REACT_APP_NETWORK_ID=8009

# Development Settings
NODE_ENV=development
DEBUG=false

# Oracle Configuration
ORACLE_ADDRESS=0x0000000000000000000000000000000000000000

# Additional Configuration
HARDHAT_NETWORK=localhost
REPORT_GAS=true
